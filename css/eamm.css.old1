.highlight {
  color: #000000;
  background-color: #EEBB00;
  border-color: #EEBB00;
  border-width: 1px 1px 1px 1px;
  border-style: solid;
}

.list {
  border-style: solid;
  border-color: #EEBB00;
  border-width: 1px 1px 1px 1px; 
  vertical-align: middle;
}

.warning {
  color: #FFB4BF;
}

img {
	border: none;
}
